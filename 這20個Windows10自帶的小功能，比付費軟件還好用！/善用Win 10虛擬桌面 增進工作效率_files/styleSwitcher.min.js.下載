function MM_findObj(d, b) { var a, c; b || (b = document); 0 < (a = d.indexOf("?")) && parent.frames.length && (b = parent.frames[d.substring(a + 1)].document, d = d.substring(0, a)); !(c = b[d]) && b.all && (c = b.all[d]); for (a = 0; !c && a < b.forms.length; a++) c = b.forms[a][d]; for (a = 0; !c && b.layers && a < b.layers.length; a++) c = MM_findObj(d, b.layers[a].document); !c && b.getElementById && (c = b.getElementById(d)); return c }
function MM_showHideLayers() { var d, b, a = MM_showHideLayers.arguments; for (d = 0; d < a.length - 2; d += 3) if (null != (b = MM_findObj(a[d]))) { var c = a[d + 2]; b.style && (b = b.style, c = "show" == c ? "visible" : "hide" == c ? "hidden" : c); if ("bool" != c) b.visibility = c; else if ("visible" == b.visibility) b.visibility = "hidden"; else if ("hidden" == b.visibility || "" == b.visibility) b.visibility = "visible" } }
function setActiveStyleSheet(d, b) { var a, c; for (a = 0; c = document.getElementsByTagName("link")[a]; a++) c.getAttribute("cat") == b && -1 != c.getAttribute("rel").indexOf("style") && c.getAttribute("title") && (c.disabled = !0, c.getAttribute("cat") == b && c.getAttribute("title") == d && (c.disabled = !1)); setActiveStyle(d, b); createCookie(b, d, 365) }
function setActiveStyle(d, b) { var a, c; for (a = 0; c = document.getElementsByTagName("a")[a]; a++) c.getAttribute("cat") == b && c.getAttribute("id") && (c.getAttribute("id") == d ? c.className = "sw-here" : c.className = ""); for (a = 0; c = document.getElementsByTagName("select")[a]; a++) if (c.getAttribute("cat") == b) for (c = c.options, a = 0; a < c.length; a++) c[a].value == d && (c[a].selected = !0) }
function getActiveStyleSheet(d) { var b, a; for (b = 0; a = document.getElementsByTagName("link")[b]; b++) if (a.getAttribute("cat") == d && -1 != a.getAttribute("rel").indexOf("style") && a.getAttribute("title") && !a.disabled) return a.getAttribute("title"); return null }
function getPreferredStyleSheet(d) { var b, a; for (b = 0; a = document.getElementsByTagName("link")[b]; b++) if (a.getAttribute("cat") == d && -1 != a.getAttribute("rel").indexOf("style") && -1 == a.getAttribute("rel").indexOf("alt") && a.getAttribute("title")) return a.getAttribute("title"); return null } function createCookie(d, b, a) { if (a) { var c = new Date; c.setTime(c.getTime() + 864E5 * a); a = "; expires=" + c.toGMTString() } else a = ""; document.cookie = d + "=" + b + a + "; path=/" }
function readCookie(d) { d += "="; for (var b = document.cookie.split(";"), a = 0; a < b.length; a++) { for (var c = b[a]; " " == c.charAt(0) ;) c = c.substring(1, c.length); if (0 == c.indexOf(d)) return c.substring(d.length, c.length) } return null } function setAllStyleCat() { var d, b; for (d = 0; b = document.getElementsByTagName("styleSwitcher")[d]; d++) if (b.getAttribute("cat")) { var a = readCookie(b.getAttribute("cat")); a = a ? a : getPreferredStyleSheet(b.getAttribute("cat")); setActiveStyleSheet(a, b.getAttribute("cat")) } }
function createAllStyleCatCookie() { var d, b; for (d = 0; b = document.getElementsByTagName("styleSwitcher")[d]; d++) if (b.getAttribute("cat")) { var a = getActiveStyleSheet(b.getAttribute("cat")); createCookie(b.getAttribute("cat"), a, 365) } };